; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="ZeldaCutsceneFramework"
run/main_scene="res://demo/cutscene_example.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"

[display]

window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="ZeldaCutsceneFramework"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg")

[internationalization]

locale/translations_pot_files=PackedStringArray("res://demo/dialogues/green_orb_dialogue.dialogue", "res://demo/dialogues/red_orb_dialogue.dialogue", "res://demo/dialogues/test_dialogue.dialogue", "res://demo/dialogues/yellow_orb_dialogue.dialogue")
